import java.util.Scanner;
import java.io.*;
class Main {
    public static void main(String[] args) 
    {
       Main obj= new Main();
        System.out.println("enter string is ");
       Scanner sc= new Scanner(System.in);
       String st=sc.nextLine();
       System.out.println("string is "+st);
      int n=st.length();
      char[] ch = st.toCharArray(); 
      System.out.println("The entire alphabets in string");
       for(int i=0;i<n;i++)
       {
           System.out.println(ch[i]);
       }
     System.out.println("The first six characters of the alphabet");
       for(int i=0;i<6;i++)
       {
           System.out.println(ch[i]);
       }
     System.out.println("The last 10 characters of the alphabets");
       for(int i=n-10;i<n;i++)
       {
           System.out.println(ch[i]);
       }
    }
}
