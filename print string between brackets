import java.util.Scanner;
import java.io.*;
class Main {
    public static void main(String[] args) 
    {
       Main obj= new Main();
        System.out.println("enter string is ");
       Scanner sc= new Scanner(System.in);
       String st=sc.nextLine();
       System.out.println("string is "+st);
      int n=st.length();
      char[] ch = st.toCharArray(); 
    int a=st.indexOf('(');
     System.out.println(a);
     int b=st.indexOf(')');
     System.out.println(b);
    System.out.println(st.substring(a+1,b));
    System.out.println(st.substring(0,a));
    System.out.println(st.substring(b+1,n));
    }
    }
